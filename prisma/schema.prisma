// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

generator client {
  provider = "prisma-client-js"
}

model Shortlist {
  id          Int    @id @default(autoincrement())
  position    String
  companyName String
  referral    String
  progress    String
}
